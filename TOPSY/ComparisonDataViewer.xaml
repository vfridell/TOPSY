<UserControl x:Class="TOPSY.ComparisonDataViewer"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300">
    <DockPanel>
        <DataGrid Name="dataGrid" Drop="dataGrid_Drop" AllowDrop="True">
            <DataGrid.Columns>
                <DataGridTextColumn Header="label" Binding="{Binding Path=label}"></DataGridTextColumn>
                <DataGridTextColumn Header="value" Binding="{Binding Path=value}"></DataGridTextColumn>
            </DataGrid.Columns>
        </DataGrid>
    </DockPanel>
</UserControl>
